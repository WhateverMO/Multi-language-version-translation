n        >\r\n        </el-input>\r\n      </li>\r\n      <li><el-button type=\"primary\" @click=\"write\">翻译图书</el-button></li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport qs from \"qs\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      kind: [\r\n        {\r\n          value: \"1\",\r\n          label: \"新闻\",\r\n        },\r\n        {\r\n          value: \"2\",\r\n          label: \"小说\",\r\n        },\r\n        {\r\n          value: \"3\",\r\n          label: \"历史\",\r\n        },\r\n        {\r\n          value: \"4\",\r\n          label: \"冒险\",\r\n        },\r\n        {\r\n          value: \"5\",\r\n          label: \"悬疑\",\r\n        },\r\n        {\r\n          value: \"6\",\r\n          label: \"爱情\",\r\n        },\r\n        {\r\n          value: \"7\",\r\n          label: \"恐怖\",\r\n        },\r\n      ],\r\n      lang: [\r\n        {\r\n          value: \"1\",\r\n          label: \"中文\",\r\n        },\r\n        {\r\n          value: \"2\",\r\n          label: \"英语\",\r\n        },\r\n        {\r\n          value: \"3\",\r\n          label: \"德语\",\r\n        },\r\n        {\r\n          value: \"4\",\r\n          label: \"法语\",\r\n        },\r\n        {\r\n          value: \"5\",\r\n          label: \"日语\",\r\n        },\r\n        {\r\n          value: \"6\",\r\n          label: \"韩语\",\r\n        },\r\n      ],\r\n      bookname: \"\",\r\n      bookintro: \"\",\r\n      bookclass: \"\",\r\n      booklang: \"\",\r\n      fristname: \"\",\r\n      fristlang: \"\",\r\n      fristintro: \"\",\r\n      firstclass: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    read() {\r\n      axios\r\n        .get(\"http://localhost:5000/author/translate_option/\" + this.bookid)\r\n        .then((res) => {\r\n          setTimeout(() => {\r\n            this.$bus.$emit(\"read\", res.data);\r\n          }, 1000);\r\n          this.$router.push(\"/readbook\");\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$bus.$on(\"bookid\", (id) => {\r\n      this.bookid = id;\r\n      this.$store.commit(\"bookid\", id);\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nul {\r\n  margin-left: 300px;\r\n  height: 60vh;\r\n  margin-top: 50px;\r\n  display: inline-block;\r\n}\r\n.new li {\r\n  margin-bottom: 25px;\r\n}\r\n.first li {\r\n  margin-bottom: 70px;\r\n}\r\n.el-input {\r\n  width: 150px;\r\n}\r\n.el-button {\r\n  /* margin-left: 600px; */\r\n}\r\n\r\n.el-textarea {\r\n  width: 250px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}