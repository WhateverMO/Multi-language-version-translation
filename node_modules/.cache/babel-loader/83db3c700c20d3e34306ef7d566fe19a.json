{"ast":null,"code":"import request from \"@/request\";\nexport default {\n  data() {\n    return {\n      books: [[]],\n      flag: false,\n      showbook: [[]],\n      pagenum: 5,\n      currentpage: 1\n    };\n  },\n\n  methods: {\n    handleSizeChange(val) {\n      this.pagenum = val;\n      console.log(`每页 ${val} 条`);\n    },\n\n    handleCurrentChange(val) {\n      this.currentpage = val;\n      var start = this.pagenum * (val - 1) + 1;\n      var end = this.pagenum * val;\n      console.log(start, end); // this.showbook = this.books.slice(\n      //  ,\n      // );\n      // console.log(this.showbook);\n\n      console.log(`当前页: ${val}`);\n    }\n\n  },\n\n  mounted() {\n    request.get(\"/api/user/read_history\").then(res => {\n      console.log(res.data);\n\n      if (this.books.length != 0) {\n        this.flag = true;\n        this.books = res.data.books;\n\n        if (res.data.books.length >= 5) {\n          this.showbook = res.data.books.slice(0, 5);\n        } else {\n          this.showbook = res.data.books;\n        }\n\n        console.log(this.showbook);\n        console.log(this.books);\n      }\n    }); //拿阅读记录\n  }\n\n};","map":{"version":3,"mappings":"AA+BA;AACA;EACAA;IACA;MACAC,WADA;MAEAC,WAFA;MAGAC,cAHA;MAIAC,UAJA;MAKAC;IALA;EAOA,CATA;;EAUAC;IACAC;MACA;MACAC;IACA,CAJA;;IAKAC;MACA;MAEA;MACA;MACAD,wBALA,CAMA;MACA;MAEA;MACA;;MACAA;IACA;;EAjBA,CAVA;;EA6BAE;IACAC;MACAH;;MACA;QACA;QACA;;QACA;UACA;QACA,CAFA,MAEA;UACA;QACA;;QACAA;QACAA;MACA;IACA,CAbA,EADA,CAcA;EACA;;AA5CA","names":["data","books","flag","showbook","pagenum","currentpage","methods","handleSizeChange","console","handleCurrentChange","mounted","request"],"sourceRoot":"src/components/information","sources":["readerread.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"this.flag\">\r\n    <div class=\"btitle\">我的阅读记录</div>\r\n    <table border=\"1\" cellspacing=\"0\">\r\n      <tr class=\"title\">\r\n        <td class=\"name\">作品名称</td>\r\n        <td class=\"author\">作者</td>\r\n        <td class=\"intro\">简介</td>\r\n        <td class=\"no\">阅读章节</td>\r\n        <td class=\"time\">阅读时间</td>\r\n      </tr>\r\n      <tr v-for=\"(items, i) in showbook\" :key=\"i\">\r\n        <td v-for=\"(item, j) in items\" :key=\"j\">{{ item }}</td>\r\n      </tr>\r\n    </table>\r\n    <div class=\"block\">\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :page-sizes=\"[5, 10]\"\r\n        :page-size=\"100\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"this.books.length\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n    <div class=\"zhan\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/request\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      books: [[]],\r\n      flag: false,\r\n      showbook: [[]],\r\n      pagenum: 5,\r\n      currentpage: 1,\r\n    };\r\n  },\r\n  methods: {\r\n    handleSizeChange(val) {\r\n      this.pagenum = val;\r\n      console.log(`每页 ${val} 条`);\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentpage = val;\r\n\r\n      var start = this.pagenum * (val - 1) + 1;\r\n      var end = this.pagenum * val;\r\n      console.log(start, end);\r\n      // this.showbook = this.books.slice(\r\n      //  ,\r\n\r\n      // );\r\n      // console.log(this.showbook);\r\n      console.log(`当前页: ${val}`);\r\n    },\r\n  },\r\n  mounted() {\r\n    request.get(\"/api/user/read_history\").then((res) => {\r\n      console.log(res.data);\r\n      if (this.books.length != 0) {\r\n        this.flag = true;\r\n        this.books = res.data.books;\r\n        if (res.data.books.length >= 5) {\r\n          this.showbook = res.data.books.slice(0, 5);\r\n        } else {\r\n          this.showbook = res.data.books;\r\n        }\r\n        console.log(this.showbook);\r\n        console.log(this.books);\r\n      }\r\n    }); //拿阅读记录\r\n  },\r\n};\r\n</script > \r\n\r\n<style scoped>\r\n