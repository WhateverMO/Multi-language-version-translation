{"ast":null,"code":"import qs from \"qs\";\nimport axios from \"axios\";\nexport default {\n  name: \"logincenter\",\n\n  data() {\n    return {\n      password: \"\",\n      user_id: \"\"\n    };\n  },\n\n  methods: {\n    enroll() {\n      this.$router.push(\"/login\");\n    },\n\n    login() {\n      var data = {\n        user_id: this.user_id,\n        password: this.password\n      };\n      const path = \"http://127.0.0.1:5000/user/login\";\n      axios.post(path, qs.stringify(data), {\n        headers: {\n          \"Content-Type\": \"application/x-www-form-urlencoded\"\n        }\n      }).then(res => {\n        alert(res.data.msg);\n\n        if (res.data.code == 200) {\n          this.$router.push(\"/reader\");\n          this.$store.commit('userid', this.user_id); //  console.log('userid',this.$store.state.userid)//显示store中userid的值\n        } else if (res.data.code == 202) {\n          this.$router.push(\"/author\");\n          this.$store.commit('authorid', this.user_id); //  console.log('userid',this.$store.state.authorid)//显示store中userid的值\n        }\n      }); //post带参数的登录请求\n      // axios.get('http://127.0.0.1:5000/user/session').then((res) => {\n      // });不带参数的get请求\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA;AACA;AACA;EACAA,mBADA;;EAEAC;IACA;MACAC,YADA;MAEAC;IAFA;EAIA,CAPA;;EAQAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;QACAH,qBADA;QAEAD;MAFA;MAIA;MACAK,MACAC,IADA,CACAC,IADA,EACAC,kBADA,EACA;QACAC;UAAA;QAAA;MADA,CADA,EAIAC,IAJA,CAIAC;QACAC;;QACA;UACA;UACA,2CAFA,CAGA;QACA,CAJA,MAIA;UACA;UACA,6CAFA,CAGA;QACA;MACA,CAfA,EANA,CAqBA;MAEA;MACA;IACA;;EA7BA;AARA","names":["name","data","password","user_id","methods","enroll","login","axios","post","path","qs","headers","then","res","alert"],"sourceRoot":"src/components/login","sources":["logincenter.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"center\">\r\n      <div class=\"center1\">\r\n        <img src=\"../../assets/img/welcom.jpeg\" />\r\n      </div>\r\n      <div class=\"