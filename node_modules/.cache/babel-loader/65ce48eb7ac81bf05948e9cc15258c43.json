>\r\n        <li @click=\"back\" class=\"change\">下一章</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      content: \"\",\r\n      title: \"\",\r\n      bookid: \"\",\r\n      contentno: \"1\",\r\n    };\r\n  },\r\n  methods: {\r\n    back() {\r\n      this.contentno = this.contentno * 1 + 1;\r\n      axios\r\n        .get(\r\n          \"http://localhost:5000/user/read_book/start_read/\" +\r\n            this.$store.state.bookid +\r\n            \"/\" +\r\n            this.contentno\r\n        )\r\n        .then((res) => {\r\n          this.content = res.data.content_text;\r\n          this.title = res.data.title;\r\n        });\r\n    },\r\n    head() {\r\n      this.contentno = this.contentno * 1 - 1;\r\n      axios\r\n        .get(\r\n          \"http://localhost:5000/user/read_book/start_read/\" +\r\n            this.$store.state.bookid +\r\n            \"/\" +\r\n            this.contentno\r\n        )\r\n        .then((res) => {\r\n          this.content = res.data.content_text;\r\n          this.title = res.data.title;\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$bus.$on(\"read\", (read) => {\r\n      this.content = read.content_text;\r\n      this.title = read.title;\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#c1 {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n}\r\n#c1 ul li {\r\n  margin-bottom: 20px;\r\n  margin-left: 50px;\r\n}\r\n#c2 {\r\n  width: 800px;\r\n  margin-left: 20vw;\r\n  height: 80vh;\r\n}\r\n#c3 {\r\n  position: fixed;\r\n  top: 50vh;\r\n  left: 15vw;\r\n}\r\n#c3 ul li {\r\n  margin-bottom: 20px;\r\n}\r\n#c4 {\r\n  position: fixed;\r\n  top: 50vh;\r\n  right: 15vw;\r\n}\r\n#c4 ul li {\r\n  margin-bottom: 20px;\r\n}\r\n.change {\r\n  cursor: pointer;\r\n}\r\n.change:hover {\r\n  color: red;\r\n}\r\n</style>"]},"metadata":{},"sourceType"