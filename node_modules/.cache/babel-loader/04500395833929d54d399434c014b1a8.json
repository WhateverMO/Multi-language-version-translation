\"http://localhost:5000/api/user/read_book/start_read/\" +\r\n            this.$store.state.bookid +\r\n            \"/\" +\r\n            this.contentno,\r\n          qs.stringify(data)\r\n        )\r\n        .then((res) => {\r\n          this.content = res.data.content_text;\r\n          this.title = res.data.title;\r\n        });\r\n    },\r\n    collect() {\r\n      axios\r\n        .post(\r\n          \"http://localhost:5000/api/user/collect/\" + this.$store.state.bookid\r\n        )\r\n        .then((res) => {\r\n          alert(res.data.msg);\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$bus.$on(\"read\", (read) => {\r\n      this.content = read.content_text;\r\n      this.title = read.title;\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.c {\r\n  background: url(../../assets/img/5.jpg);\r\n  background-repeat: no-repeat;\r\n  background-attachment: fixed;\r\n  background-position: center;\r\n  width: 90vw;\r\n  margin: 0 auto;\r\n}\r\n}\r\n.content {\r\n  width: 55vw;\r\n  margin: 50px auto;\r\n  border: 1px solid black;\r\n\r\n  padding: 40px;\r\n  font-family: \"Courier New\", Courier, monospace;\r\n  font-size: 20px;\r\n  letter-spacing: 5px;\r\n}\r\n.zhan {\r\n  height: 50px;\r\n}\r\n#c1 {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n}\r\n#c1 ul li {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n#c3 {\r\n  position: fixed;\r\n  top: 50vh;\r\n  left: 15vw;\r\n}\r\n#c3 ul li {\r\n  margin-bottom: 20px;\r\n}\r\n#c4 {\r\n  position: fixed;\r\n  top: 50vh;\r\n  right: 15vw;\r\n}\r\n#c4 ul li {\r\n  margin-bottom: 20px;\r\n}\r\n.change {\r\n  cursor: pointer;\r\n}\r\n.change:hover {\r\n  color: red;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}