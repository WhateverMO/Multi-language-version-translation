 request.post(\"/api/author/add_books_cover/1\", this.formData).then(res => {\n        alert(res.data.msg);\n      });\n    },\n\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === \"image/jpeg\";\n      const isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG) {\n        this.$message.error(\"上传头像图片只能是 JPG 格式!\");\n      }\n\n      if (!isLt2M) {\n        this.$message.error(\"上传头像图片大小不能超过 2MB!\");\n      }\n\n      return isJPG && isLt2M;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoEA;AACA;AACA;EACAA;IACA;MACAC,OACA;QACAC,UADA;QAEAC;MAFA,CADA,EAKA;QACAD,UADA;QAEAC;MAFA,CALA,EASA;QACAD,UADA;QAEAC;MAFA,CATA,EAaA;QACAD,UADA;QAEAC;MAFA,CAbA,EAiBA;QACAD,UADA;QAEAC;MAFA,CAjBA,EAqBA;QACAD,UADA;QAEAC;MAFA,CArBA,EAyBA;QACAD,UADA;QAEAC;MAFA,CAzBA,CADA;MA+BAC,OACA;QACAF,UADA;QAEAC;MAFA,CADA,EAKA;QACAD,UADA;QAEAC;MAFA,CALA,EASA;QACAD,UADA;QAEAC;MAFA,CATA,EAaA;QACAD,UADA;QAEAC;MAFA,CAbA,EAiBA;QACAD,UADA;QAEAC;MAFA,CAjBA,EAqBA;QACAD,UADA;QAEAC;MAFA,CArBA,CA/BA;MAyDAE,YAzDA;MA0DAC,aA1DA;MA2DAC,aA3DA;MA4DAC,YA5DA;MA6DAC,uBA7DA;MA8DAC;IA9DA;EAgEA,CAlEA;;EAmEAC;IACAC;MACA;QACAC,mBADA;QAEAC,2CAFA;QAGAC,yBAHA;QAIAC,0CAJA;QAKAP;MALA;MAOAQ;MACA;MACAA;MACAA;MACAC;QACAC;;QACA;UACAA;UACAC;YACA;UACA,CAFA,EAEA,IAFA;UAGA;QACA;MACA,CATA;IAUA,CAvBA;;IAwBAC;MACA;MACA;MACAH,QACAI,IADA,CACA,+BADA,EACA,aADA,EAEAC,IAFA,CAEAC;QACAL;MACA,CAJA;IAKA,CAhCA;;IAiCAM;MACA;MACA;;MAEA;QACA;MACA;;MACA;QACA;MACA;;MACA;IACA;;EA5CA;AAnEA","names":["data","kind","value","label","lang","bookname","bookintro","bookclass","booklang","picture","imageUrl","methods","write","name","lang_id","book_desc","bc_id","console","request","alert","setTimeout","handleAvatarSuccess","post","then","res","beforeAvatarUpload"],"sourceRoot":"src/components/addbook","sources":["kindcenter.vue"],"sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <ul>\r\n      <li>\r\n        图书名字：<el-input\r\n          v-model=\"bookname\"\r\n          placeholder=\"请输入图书名字\"\r\n        ></el-input>\r\n      </li>\r\n      <li>\r\n        作品类型：<el-select\r\n          v-model=\"bookclass\"\r\n          ref=\"bookclass\"\r\n          placeholder=\"请选择\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in kind\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n      </li>\r\n      <li>\r\n        语言种类：<el-select\r\n          v-model=\"booklang\"\r\n          ref=\"booklang\"\r\n          placeholder=\"请选择\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in lang\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n      </li>\r\n      <li>\r\n        作品简介:\r\n        <el-input\r\n          type=\"textarea\"\r\n          :rows=\"4\"\r\n          :maxlength=\"40\"\r\n          placeholder=\"请输入内容\"\r\n          v-model=\"bookintro\"\r\n        >\r\n        </el-input>\r\n      </li>\r\n      <li><el-button type=\"primary\" @click=\"write\">创建图书</el-button></li>\r\n    </ul>\r\n    <ul>\r\n      <el-upload\r\n        class=\"avatar-uploader\"\r\n        action=\"http://localhost:5000/api/author/add_books_index\"\r\n        :show-file-list=\"false\"\r\n        :on-success=\"handleAvatarSuccess\"\r\n        :before-upload=\"beforeAvatarUpload\"\r\n      >\r\n        <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\r\n        <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n      </el-upload>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport qs from \"qs\";\r\nimport request from \"@/request\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      kind: [\r\n        {\r\n          value: \"1\",\r\n          label: \"新闻\",\r\n        },\r\n        {\r\n          value: \"2\",\r\n          label: \"小说\",\r\n        },\r\n        {\r\n          value: \"3\",\r\n          label: \"历史\",\r\n        },\r\n        {\r\n          value: \"4\",\r\n          label: \"冒险\",\r\n        },\r\n        {\r\n          value: \"5\",\r\n          label: \"悬疑\",\r\n        },\r\n        {\r\n          value: \"6\",\r\n          label: \"爱情\",\r\n        },\r\n        {\r\n          value: \"7\",\r\n          label: \"恐怖\",\r\n        },\r\n      ],\r\n      lang: [\r\n        {\r\n          value: \"1\",\r\n          label: \"中文\",\r\n        },\r\n        {\r\n          value: \"2\",\r\n          label: \"英语\",\r\n        },\r\n        {\r\n          value: \"3\",\r\n          label: \"德语\",\r\n        },\r\n        {\r\n          value: \"4\",\r\n          label: \"法语\",\r\n        },\r\n        {\r\n          value: \"5\",\r\n          label: \"日语\",\r\n        },\r\n        {\r\n          value: \"6\",\r\n          label: \"韩语\",\r\n        },\r\n      ],\r\n      bookname: \"\",\r\n      bookintro: \"\",\r\n      bookclass: \"\",\r\n      booklang: \"\",\r\n      picture: new FormData(),\r\n      imageUrl: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    write() {\r\n      var data = {\r\n        name: this.bookname,\r\n        lang_id: this.$refs.booklang.selected.value,\r\n        book_desc: this.bookintro,\r\n        bc_id: this.$refs.bookclass.selected.value,\r\n        picture: this.picture,\r\n      };\r\n      console.log(this.picture);\r\n      const path = \"/api/author/add_books_index\";\r\n      console.log(data);\r\n      console.log(qs.stringify(data));\r\n      request.post(path, data).then((res) => {\r\n        alert(res.data.msg);\r\n        if (res.data.code == 200) {\r\n          alert(\"你的图书编号是\" + res.data.book_id);\r\n          setTimeout(() => {\r\n            this.$bus.$emit(\"createid\", res.data.book_id);\r\n          }, 1000);\r\n          this.$router.push(\"/write\");\r\n        }\r\n      });\r\n    },\r\n    handleAvatarSuccess(res, files) {\r\n      this.imageUrl = URL.createObjectURL(files.raw);\r\n      this.picture.append(\"file\", files.raw, files.name);\r\n      request\r\n        .post(\"/api/author/add_books_cover/1\", this.formData)\r\n        .then((res) => {\r\n          alert(res.data.msg);\r\n        });\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === \"image/jpeg\";\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error(\"上传头像图片只能是 JPG 格式!\");\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 2MB!\");\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.box {\r\n  margin-left: 35vh;\r\n  margin-top: 80px;\r\n}\r\nul {\r\n  margin-left: 100px;\r\n  float: left;\r\n}\r\nul li {\r\n  margin-bottom: 30px;\r\n}\r\n.avatar-uploader {\r\n  cursor: pointer;\r\n  width: 278px;\r\n  height: 278px;\r\n  background-color: rgb(235, 252, 247);\r\n  border: 1px dashed #7a7b7b;\r\n}\r\n.avatar-uploader:hover {\r\n  border: 1px dashed #c93b28;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 48px;\r\n  width: 278px;\r\n  height: 278px;\r\n  line-height: 278px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 278px;\r\n  height: 278px;\r\n  display: block;\r\n}\r\n.el-input {\r\n  width: 150px;\r\n}\r\n.el-button {\r\n  margin-left: 5vw;\r\n}\r\n\r\n.el-textarea {\r\n  width: 250px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}