{"ast":null,"code":"import request from \"@/request\";\nexport default {\n  data() {\n    return {\n      books: [[]],\n      flag: false,\n      showbook: [[]],\n      pagenum: 5,\n      currentpage: 1\n    };\n  },\n\n  methods: {\n    handleSizeChange(val) {\n      this.pagenum = val;\n    },\n\n    handleCurrentChange(val) {\n      this.currentpage = val;\n      var start = this.pagenum * (val - 1) + 1;\n      var end = this.pagenum * val;\n      console.log(start, end);\n\n      if (end <= this.books.length) {\n        this.showbook = this.books.slice(start, end);\n      } else {\n        this.showbook = this.books.slice(start, this.books.length);\n        console.log(this.showbook);\n      }\n    }\n\n  },\n\n  mounted() {\n    request.get(\"/api/user/read_history\").then(res => {\n      console.log(res.data);\n\n      if (this.books.length != 0) {\n        this.flag = true;\n        this.books = res.data.books;\n\n        if (res.data.books.length >= 5) {\n          this.showbook = res.data.books.slice(0, 4);\n        } else {\n          this.showbook = res.data.books;\n        }\n\n        console.log(this.showbook);\n        console.log(this.books);\n      }\n    }); //拿阅读记录\n  }\n\n};","map":{"version":3,"mappings":"AA+BA;AACA;EACAA;IACA;MACAC,WADA;MAEAC,WAFA;MAGAC,cAHA;MAIAC,UAJA;MAKAC;IALA;EAOA,CATA;;EAUAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;MACA;MACA;MACAC;;MACA;QACA;MACA,CAFA,MAEA;QACA;QACAA;MACA;IACA;;EAfA,CAVA;;EA2BAC;IACAC;MACAF;;MACA;QACA;QACA;;QACA;UACA;QACA,CAFA,MAEA;UACA;QACA;;QACAA;QACAA;MACA;IACA,CAbA,EADA,CAcA;EACA;;AA1CA","names":["data","books","flag","showbook","pagenum","currentpage","methods","handleSizeChange","handleCurrentChange","console","mounted","request"],"sourceRoot":"src/components/information","sources":["readerread.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"this.flag\">\r\n    <div class=\"btitle\">我的阅读记录</div>\r\n    <table border=\"1\" cellspacing=\"0\">\r\n      <tr class=\"title\">\r\n        <td class=\"name\">作品名