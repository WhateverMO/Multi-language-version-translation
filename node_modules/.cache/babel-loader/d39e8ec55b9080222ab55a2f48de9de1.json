st:5000/api\").then(res => {\n      this.$bus.$emit(\"books\", res.data.books);\n    }); //给首页发请求拿数据\n\n    this.$bus.$on(\"books\", books => {\n      this.books = books;\n    });\n  }\n\n};","map":{"version":3,"mappings":"AA+UA;AACA;AACA;EACAA,cADA;;EAEAC;IACA;MACAC,SADA;MAEAC,UACA;QAAAC;QAAAC;MAAA,CADA,EAEA;QAAAD;QAAAC;MAAA,CAFA,EAGA;QAAAD;QAAAC;MAAA,CAHA,CAFA;MAOAC,WAPA;MAQAC,OARA;MASAC;IATA;EAWA,CAdA;;EAeAC;IACAC;MACAC,MACAC,GADA,CAEA,wDACA,gBAHA,EAKAC,IALA,CAKAC;QACAC;UACA;UACA;QACA,CAHA,EAGA,IAHA;QAIA;MACA,CAXA,EADA,CAYA;IACA,CAdA;;IAeAC;MACAL,MACAC,GADA,CAEA,wDACA,gBAHA,EAKAC,IALA,CAKAC;QACAC;UACA;UACA;QACA,CAHA,EAGA,IAHA;QAIA;MACA,CAXA,EADA,CAYA;IACA,CA5BA;;IA6BAE;MACAN,MACAC,GADA,CAEA,wDACA,gBAHA,EAKAC,IALA,CAKAC;QACAC;UACA;UACA;QACA,CAHA,EAGA,IAHA;QAIA;MACA,CAXA,EADA,CAYA;IACA,CA1CA;;IA2CAG;MACAP,MACAC,GADA,CAEA,wDACA,gBAHA,EAKAC,IALA,CAKAC;QACAC;UACA;UACA;QACA,CAHA,EAGA,IAHA;QAIA;MACA,CAXA,EADA,CAYA;IACA;;EAxDA,CAfA;;EA0EAI;IACAR;MACA;IACA,CAFA,EADA,CAGA;;IACA;MACA;IACA,CAFA;EAGA;;AAjFA","names":["name","data","input","imglist","id","image","books","flag","textarea","methods","intro1","axios","get","then","res","setTimeout","intro2","intro3","intro4","mounted"],"sourceRoot":"src/components/index","sources":["center.vue"],"sourcesContent":["<template>\r\n  <div class=\"all\">\r\n    <div class=\"choose\">\r\n      <ul>\r\n        <li>{{ $t(\"m.Classificationofworks\") }}</li>\r\n        <li>{{ $t(\"m.ranking\") }}</li>\r\n        <li>{{ $t(\"m.Endofbook\") }}</li>\r\n        <li>{{ $t(\"m.free\") }}</li>\r\n        <li>{{ $t(\"m.WritersSection\") }}</li>\r\n        <li>{{ $t(\"m.Recommendedzone\") }}</li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"center\">\r\n      <el-carousel :interval=\"6000\" arrow=\"always\" height=\"50vh\">\r\n        <el-carousel-item v-for=\"item in imglist\" :key=\"item.id\">\r\n          <h3><img :src=\"item.image\" /></h3>\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n    </div>\r\n    <div class=\"reconmend\"